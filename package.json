{
  "name": "awesome-testing",
  "version": "1.0.0",
  "description": "A curated list of testing resources",
  "main": "index.js",
  "scripts": {
    "test": "remark README.md -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/irshadpc/awesome-testing.git"
  },
  "keywords": [
    "testing",
    "curated list"
  ],
  "author": "TheJambo",
  "license": "Creative Commons Zero v1.0 Universal (CC0 1.0 Universal)",
  "bugs": {
    "url": "https://github.com/irshadpc/awesome-testing/issues"
  },
  "homepage": "https://github.com/irshadpc/awesome-testing#readme",
  "dependencies": {
    "remark": "^8.0.0",
    "remark-cli": "^4.0.0",
    "remark-lint": "^6.0.0",
    "remark-lint-alphabetize-lists": "^2.0.0",
    "remark-lint-are-links-valid-alive": "^0.2.1",
    "remark-lint-are-links-valid-duplicate": "^0.2.1",
    "remark-preset-lint-recommended": "^3.0.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "space"
      ],
      "remark-lint-alphabetize-lists",
      "remark-lint-are-links-valid-duplicate",
      "remark-lint-are-links-valid-alive"
    ],
    "settings": {
      "commonmark": true
    }
  }
}
